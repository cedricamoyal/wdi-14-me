
<%= form_for @plane do |f| %>

<div class='col-md-6 col-md-offset-3'>
  <div class="stepsDetailsShow specialForm">

  <p>
    <%= f.label :name, :class => "labels", :class => "label1" %>
    <%= f.text_field :name, :required => true, :class => "inputs", :class => "label2" %>
  </p>

  <p>
    <%= f.label :rows, :class => "labels", :class => "label1" %>
    <%= f.text_field :rows, :required => true, :class => "inputs", :class => "label2" %>
  </p>

  <p>
    <%= f.label :columns, :class => "labels", :class => "label1" %>
    <%= f.text_field :columns, :required => true, :class => "inputs", :class => "label2" %>
  </p>

<br>

  <%= f.submit %>

</div>
</div>

<% end %>
